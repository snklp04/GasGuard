{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "gasguard_frontend/dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/gasguard_frontend/dist/assets/$1" },
    { "src": "/(.*)", "dest": "/gasguard_frontend/dist/index.html" }
  ]
}
